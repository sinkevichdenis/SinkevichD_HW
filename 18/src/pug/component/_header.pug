mixin menuLink(name, adress)
	a(href= adress).header-link= name

div.header
	span.header-logo
		a(href="#") 
			img(src="https://www.freelogodesign.org/Content/img/logo-ex-7.png")
	div.header-links__current
		+menuLink("Direct", "#")
		+menuLink("Some", "#")
		+menuLink("Other", "#")
		+menuLink("Shop", "#")
		+menuLink("Feedbacks", "#")
		+menuLink("FAQ", "#")
	div.header-button
		+menuLink("=", "#")
